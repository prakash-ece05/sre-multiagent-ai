# Google Gemini AI Configuration
GOOGLE_API_KEY=your_gemini_api_key_here

# Grafana Configuration
GRAFANA_URL=https://your-grafana-instance.com
GRAFANA_API_TOKEN=your_grafana_api_token_here
GRAFANA_ORG_ID=1

# Prometheus Data Source (via Grafana)
PROMETHEUS_DATASOURCE_ID=2

# Tempo Data Source (via Grafana)
TEMPO_DATASOURCE_ID=3
TEMPO_URL=https://your-tempo-instance.com

# Apollo Router Configuration
APOLLO_ROUTER_URL=https://your-apollo-router.com
APOLLO_ADMIN_API_KEY=your_apollo_admin_api_key_here

# Harness Configuration
HARNESS_URL=https://app.harness.io
HARNESS_API_KEY=your_harness_api_key_here
HARNESS_ACCOUNT_ID=your_account_id_here

# Application Configuration
LOG_LEVEL=INFO
SESSION_TIMEOUT_MINUTES=30
MAX_RETRY_ATTEMPTS=3
API_TIMEOUT_SECONDS=8

# Failover Safety Configuration
FAILOVER_RATE_LIMIT_SECONDS=300
FAILOVER_REQUIRE_HEALTH_CHECK=true
FAILOVER_ENABLE_ROLLBACK=true

# Feature Flags
ENABLE_DEPLOYMENT_AGENT=true
ENABLE_TRACE_AGENT=true
ENABLE_FAILOVER_AGENT=true
